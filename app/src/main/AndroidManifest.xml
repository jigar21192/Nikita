<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.user.ims_i_can">

    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/can1"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".SplashScreen">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".LogIn" />
        <activity android:name=".Registration" />
        <activity android:name=".OTP" />
        <activity android:name=".Forget_pwd" />
        <activity android:name=".Registration_Detail" />
        <activity
            android:name=".Drawer"
            android:label="@string/title_activity_drawer"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Faculty_Registration" />
        <activity android:name=".Faculty_Detail" />
        <activity android:name=".Faculty_Login" />
        <activity android:name=".Faculty_Dashboard" />
        <activity android:name=".Faculty_OTP" />
        <activity android:name=".Faculty_Forget_Password" />
        <activity android:name=".Dashboard" />
        <activity android:name=".Signup" />
        <activity android:name=".Inquiry" />
        <activity
            android:name=".Welcome_page"
            android:label="@string/title_activity_welcome_page"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity
            android:name=".Ranker_list"
            android:label="@string/title_activity_ranker_list"
            android:theme="@style/AppTheme.NoActionBar" />
        <activity android:name=".Syllabus"></activity>
    </application>

</manifest>